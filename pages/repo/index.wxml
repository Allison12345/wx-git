<!-- @format -->

<my-page>
  <repo-item
    wx:for="{{ repoItems }}"
    wx:key="*this"
    login="{{ item.owner.login }}"
    name="{{ item.name }}"
    desc="{{ item.description }}"
    lang="{{ item.language }}"
    update="{{ item.updated_at }}"
    forks="{{ item.forks }}"
  ></repo-item>
  <page-bottom></page-bottom>
</my-page>
